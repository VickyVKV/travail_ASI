Publisher: SAGE Publications},
	keywords = {Activism, Activists, Deconstruction, Everyday life, Internet, Italy, Media, Political Economy, Practice, Programming (Broadcast), Social Environment, Social movements, Students, Youth Movements},
	pages = {1269--1285},
}


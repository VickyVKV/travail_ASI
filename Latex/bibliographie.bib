	note = {Number: 1
Publisher: Nature Publishing Group},
	keywords = {Information theory and computation, Statistical physics, thermodynamics and nonlinear dynamics},
	pages = {20902},
	file = {Full Text PDF:C\:\\Users\\vkvvi\\Zotero\\storage\\8KCBULMV\\Nadini et al. - 2021 - Mapping the NFT revolution market trends, trade n.pdf:application/pdf;Snapshot:C\:\\Users\\vkvvi\\Zotero\\storage\\KKGS8F7A\\s41598-021-00053-8.html:text/html},
